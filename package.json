{
  "name": "express-mongoose-angular2",
  "version": "0.1.0",
  "description": "A Boilerplate application for building fullstack app with express, mongoose backend and angular2 frontend",
  "author": "Bartek Cichocki <bartek@codinarium.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9",
    "yarn": ">=0.17.9"
  },
  "scripts": {
    "protractor": "protractor",
    "tslint": "tslint",
    "webdriver-manager": "webdriver-manager",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.webpack.json",
    "build": "yarn server.build && yarn client.build:aot",
    "test": "yarn server.test && yarn client.test",
    "lint": "yarn server.lint && yarn client.lint",
    "start": "npm server.start",
    "server.start": "gulp serve",
    "server.build": "gulp",
    "server.lint": "esw *.js server/**/*.js config/server/**/*.js --color",
    "server.lint:watch": "yarn server.lint -- --watch",
    "server.test": "cross-env NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "server.test2": "gulp mocha",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "client": "yarn client.start",
    "client.clean:dist": "rimraf dist/client",
    "client.clean:aot": "rimraf compiled",
    "client.start": "webpack-dev-server --config config/client/webpack.dev.js --progress --profile --watch --content-base client/src/ ",
    "client.lint": "yarn tslint \"src/**/*.ts\"",
    "client.karma": "karma start",
    "client.test": "yarn client.karma && yarn client.e2e",
    "client.build:aot": "yarn client.clean:dist && yarn client.clean:aot && yarn ngc && webpack --config config/client/webpack.prod.js  --progress --profile --bail",
    "client.build": "yarn client.clean.dist && webpack --config config/client/webpack.prod.js  --progress --profile --bail",
    "preclient.e2e": "yarn webdriver-manager update -- --standalone",
    "client.e2e": "yarn protractor",
    "cliente2e:live": "yarn e2e -- --elementExplorer"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bartcich/express-mongoose-angular2.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular/material": "2.0.0-alpha.10",
    "@angularclass/conventions-loader": "^1.0.2",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "assets-webpack-plugin": "^3.4.0",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "~5.0.2",
    "zone.js": "~0.7.4",

    "http-server": "^0.9.0",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "jsonwebtoken": "7.1.9",
    "method-override": "^2.3.5",
    "mongoose": "4.7.7",
    "morgan": "1.7.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.1",
    "@types/core-js": "^0.9.34",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.2.34",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "~2.53.39",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^2.0.0",
    "angular-router-loader": "^0.4.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "~3.0.0-beta.17",
    "codelyzer": "~1.0.0-beta.4",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "1.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "3.12.2",
    "parse5": "^2.2.2",
    "protractor": "^4.0.14",
    "raw-loader": "0.5.1",
    "rimraf": "~2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "sass-loader": "4.0.2",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "~4.3.1",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.0",
    "typescript": "2.1.1",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.2.0-rc.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~1.0.1",

    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "eslint": "3.12.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "ghooks": "^1.2.4",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-istanbul": "1.1.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-mocha": "3.0.1",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "isparta": "4.0.0",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "license": "MIT",
  "config": {
    "ghooks": {
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
